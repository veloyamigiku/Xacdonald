<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation"
    app:startDestination="@id/HomeFragment">

    <custom_fragment
        android:id="@+id/HomeFragment"
        android:name="jp.co.myself.xacdonald.fragment.HomeFragment"
        android:label="HomeFragment" />
    <custom_fragment
        android:id="@+id/CouponFragment"
        android:name="jp.co.myself.xacdonald.fragment.CouponFragment"
        android:label="CouponFragment" />
    <custom_fragment
        android:id="@+id/MenuFragment"
        android:name="jp.co.myself.xacdonald.fragment.MenuFragment"
        android:label="MenuFragment" >
        <action
            android:id="@+id/action_MenuFragment_to_MenuDetailFragment"
            app:destination="@id/MenuDetailFragment" />
    </custom_fragment>
    <custom_fragment
        android:id="@+id/MenuDetailFragment"
        android:name="jp.co.myself.xacdonald.fragment.MenuDetailFragment"
        android:label="MenuDetailFragment" >
        <argument
            android:name="menuItem"
            app:argType="jp.co.myself.xacdonald.model.view.menu.MenuItem" />
        <action
            android:id="@+id/action_MenuDetailFragment_to_MenuInfoDetailFragment"
            app:destination="@id/MenuInfoDetailFragment" />
    </custom_fragment>
    <custom_fragment
        android:id="@+id/MenuInfoDetailFragment"
        android:name="jp.co.myself.xacdonald.fragment.MenuInfoDetailFragment"
        android:label="MenuInfoDetailFragment" >
        <argument
            android:name="menuItem"
            app:argType="jp.co.myself.xacdonald.model.view.menu.MenuItem" />
    </custom_fragment>


</navigation>
